  "license": "ISC",
  "version": "1.0.0",
  "description": "A React App",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev-server lint:watch",
    "dev-server": "babel-node server/devServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src server",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node server/build.js",
    "postbuild": "babel-node server/distServer.js",
    "remove-gzip": "node_modules/.bin/rimraf dist_gzip",
    "precompress": "npm run remove-gzip && mkdir dist_gzip",
    "compress": "babel-node server/compress.js"
  },
  "keywords": [
    "react",
    "redux",
    "sass",
    "eslint",
    "webpack",
    "bootstrap",
    "jQuery"
  ],
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-equimper": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "bootstrap": "^3.3.7",
    "compress": "^0.99.0",
    "compression": "^1.7.1",
    "css-loader": "^0.28.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "15.4.2",
    "react-fontawesome": "^1.6.1",
    "react-input": "^4.1.3",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "tether": "^1.4.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
